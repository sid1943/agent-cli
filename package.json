{
  "name": "@anthropic/agent-coordinator",
  "version": "1.0.0",
  "description": "Multi-terminal AI agent coordination system for parallel development",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agent": "./bin/agent.js",
    "agent-server": "./bin/server.js",
    "agent-client": "./bin/client.js",
    "agent-status": "./bin/status.js",
    "agent-task": "./bin/task.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node bin/agent.js",
    "server": "node bin/server.js",
    "client": "node bin/client.js",
    "status": "node bin/status.js",
    "task": "node bin/task.js",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "ai",
    "agent",
    "coordination",
    "parallel",
    "development",
    "claude"
  ],
  "author": "Anthropic",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "chokidar": "^3.5.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
